// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Account {
  id                String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt         DateTime @default(now())
  updatedAt         DateTime @updatedAt
  userId            String   @map("user_id")
  type              String
  provider          String
  providerAccountId String   @map("provider_account_id")
  refresh_token     String?
  access_token      String?
  expires_at        Int?
  token_type        String?
  scope             String?
  id_token          String?  @db.ObjectId
  session_state     String?

  user User @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([provider, providerAccountId])
  @@map("accounts")
}

model Session {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
  sessionToken String   @unique @map("session_token")
  userId       String   @map("user_id")
  expires      DateTime
  user         User     @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@map("sessions")
}

model User {
  id           String    @id @default(auto()) @map("_id") @db.ObjectId
  createdAt    DateTime  @default(now())
  updatedAt    DateTime  @updatedAt
  email        String    @unique
  firstName    String?   @map("first_name")
  lastName     String?   @map("last_name")
  address      String?
  zipCode      String?   @map("zip_code")
  city         String?
  state        String?
  country      String?
  mobile       String?
  status       String?
  gender       String?
  age          String?
  userPassword String    @map("user_password")
  role         String?
  accounts     Account[]
  sessions     Session[]

  @@map("tb_user")
}

model Company {
  id             String    @id @default(auto()) @map("_id") @db.ObjectId
  createdAt      DateTime  @default(now())
  updatedAt      DateTime  @updatedAt
  name           String?
  ownerName      String?   @map("owner_Name")
  pic            String?
  country_code   String?
  description    String?
  address        String?
  streetAddress  String?   @map("street_address")
  city           String?
  state          String?
  zipCode        String?   @map("zip_code")
  country        String?
  email          String?   @unique
  mobile         String?
  pincode        String?
  gender         String?
  accountType    String?   @map("account_type")
  locLatitude    Float?    @map("loc_lat")
  locLongitude   Float?    @map("loc_lon")
  locAltitude    Float?    @default(0) @map("loc_alt")
  registerNo     String?   @map("register_no")
  website        String?
  foundationYear String?
  status         String?
  tagLine        String?   @map("tag_line")
  projects       Project[]

  @@map("tb_company")
}

model Project {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
  name         String   @unique
  status       String?  @map("status")
  state        String?
  country      String?
  customerName String?  @map("customer_name")
  application  String?  @map("application")
  companyId    String?  @map("company_id") @db.ObjectId
  company      Company? @relation(fields: [companyId], references: [id])
  projectJson  Json?

  @@map("tb_project")
}

model Roles {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  name        String   @unique
  description String   @unique
  code        String   @unique
  status      String?

  @@map("tb_roles")
}

model Widgets {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  name        String?
  code        String?
  description String?
  query       String?
  image       String?

  @@map("tb_widgets")
}

model Template {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  cell      String?

  @@map("tb_template")
}

model DashboardTemplate {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  name        String?  @map("template_name")
  status      String?  @map("status")
  description String?  @map("description")
  row         Int?     @map("row")

  @@map("tb_dashboard_template")
}

model Page {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  name      String?
  type      String?
  image     String?

  @@map("tb_page")
}

model Theme {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  name      String?
  type      String?
  image     String?

  @@map("tb_theme")
}

model Customer {
  id            String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
  profilePic    String?
  country_code  String?
  firstName     String?  @map("first_name")
  lastName      String?  @map("last_name")
  address       String?
  city          String?
  state         String?
  zipCode       String?  @map("zip_code")
  country       String?
  mobile        String?
  streetAddress String?  @map("street_address")
  status        String?
  gender        String?
  age           String?
  email         String?  @unique

  @@map("tb_customer")
}

model Component {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  name      String

  @@map("tb_component")
}

model VerificationToken {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
  identifier String
  token      String   @unique
  expires    DateTime

  @@unique([identifier, token])
  @@map("tb_verification_tokens")
}
